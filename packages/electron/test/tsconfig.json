{
	"compilerOptions": {
		"baseUrl": ".",
		"tsBuildInfoFile": ".tmp/tsbuildinfo.json",
		"module": "es2020",
		"target": "es2020",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"sourceMap": true,
		"declaration": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"incremental": true,
		"removeComments": false,
		"composite": true,
		"types": ["node", "jest"]
	},
	"references": [
		{
			"path": ".."
		}
	]
}
